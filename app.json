{
    "expo": {
        "name": "Memorize That Wall",
        "slug": "memorize-that-wall",
        "version": "1.1.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "memorizethatwall",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#1e1e1e"
            },
            "edgeToEdgeEnabled": true,
            "package": "nl.r1a.memorizethatwall"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#1e1e1e"
                }
            ],
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app needs access to your photos to let you select images of Walls."
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "448e3eb1-f41a-463d-a293-73d75d0f6094"
            }
        }
    }
}
